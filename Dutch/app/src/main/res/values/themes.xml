<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="all_title">
        <item name="android:lineSpacingExtra">2dp</item>

        <item name="android:textSize">24sp</item>
        <item name="android:textColor">@color/textview_textcolor_white</item>
    </style>

    <style name="Theme.Dutch" parent="Theme.AppCompat.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>


    <style name="Theme.Splash" parent="Theme.SplashScreen">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="windowSplashScreenBackground">@color/black_splash</item>
        <item name="windowSplashScreenAnimatedIcon">@drawable/splash_logo</item>
    </style>

    <style name="Pretendard_Bold">
        <item name="android:fontFamily">@font/pretendard_bold</item>
    </style>

    <style name="Pretendard_SemiBold">
        <item name="android:fontFamily">@font/pretendard_semibold</item>
    </style>

    <style name="Pretendard_Medium">
        <item name="android:fontFamily">@font/pretendard_medium</item>
    </style>

    <style name="Pretendard_Regular">
        <item name="android:fontFamily">@font/pretendard_regular</item>
    </style>
</resources>